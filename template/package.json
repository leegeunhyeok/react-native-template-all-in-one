{
  "name": "RNTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "android": "PLATFORM=native react-native run-android",
    "ios": "PLATFORM=native react-native run-ios",
    "lint": "eslint .",
    "start:native": "PLATFORM=native react-native start",
    "test:native": "PLATFORM=native yarn test",
    "start:web": "PLATFORM=web react-scripts start",
    "build:web": "PLATFORM=web react-scripts build",
    "test:web": "PLATFORM=web react-scripts test --env=jsdom",
    "eject:web": "react-scripts eject",
    "start:storybook:native": "STORYBOOK=1 yarn start:native",
    "start:storybook:web": "PLATFORM=web storybook dev --port 6006",
    "build:storybook:web": "PLATFORM=web storybook build --output-dir dist-storybook",
    "update-stories": "sb-rn-get-stories --config-path .ondevice",
    "storybook-watcher": "sb-rn-watcher --config-path .ondevice"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.1",
    "@react-native-community/datetimepicker": "^7.4.1",
    "@react-native-community/slider": "^4.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.3",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-web": "^0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@storybook/addon-actions": "^7.1.0",
    "@storybook/addon-actions-legacy": "npm:@storybook/addon-actions@^6.5.14",
    "@storybook/addon-controls": "^7.1.0",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/addon-ondevice-actions": "^6.5.4",
    "@storybook/addon-ondevice-backgrounds": "^6.5.4",
    "@storybook/addon-ondevice-controls": "^6.5.4",
    "@storybook/addon-ondevice-notes": "^6.5.4",
    "@storybook/addon-react-native-web": "^0.0.21",
    "@storybook/cli": "^7.1.0",
    "@storybook/react": "^7.1.0",
    "@storybook/react-native": "^6.5.4",
    "@storybook/react-webpack5": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-react-native-web": "^0.19.6",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.7",
    "patch-package": "^7.0.2",
    "prettier": "^2.4.1",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "18.2.0",
    "storybook": "^7.1.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
